pipeline {
    agent any
    stages {
        stage('Checkout Code') {
            steps {
                checkout scm
                // OR specify repository details explicitly:
                git branch: 'main',
                credentialsId: 'git_pipeline',
                url: 'https://github.com/developer-markantony/pipeline.git'
		sh '''
		echo "presentation done"
		'''
            }	
        }
    }
}
